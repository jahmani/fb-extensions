{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
    "dataGroups": [{
    "name": "api-freshness",
    "urls": [
      "https://storage.googleapis.com/store-gallary.appspot.com/stores/*/productPhotos/thumbs/*"
    ],
    "cacheConfig": {
      "maxSize": 3000,
      "maxAge": "30d",
      "timeout": "1s",
      "strategy": "performance"
    }
  }]
}
